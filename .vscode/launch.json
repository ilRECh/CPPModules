	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/Module01/ex04/replace",
			"args": [
				"file1",
				"hello",
				"general"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/Module01/ex04/",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}